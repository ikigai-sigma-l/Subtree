<li class="avplayer-ui-folder-list-li" role="listitem">
  <div class="avplayer-ui-folder-node {{#if node.played}}active{{/if}}" style="padding-inline-start: {{paddingStart(node)}}px;">
    {{#if isFolder(node)}}
      <div class="avplayer-ui-folder-expand hint--right" data-open="{{node.opened ? 1 : 0}}" on-click="toggle()" data-hint="{{ node.opened ? language.TIP_FOLD : language.TIP_UNFOLD}}" >
        <svg viewBox="0 0 24 24">
          <path d="m10 17 5-5-5-5v10z"></path>
        </svg>
      </div>
      <div class="avplayer-ui-folder-icon dir">
        <svg viewBox="0 0 24 24">
          <path d="M10 4H4c-1.1 0-1.99.9-1.99 2L2 18c0 1.1.9 2 2 2h16c1.1 0 2-.9 2-2V8c0-1.1-.9-2-2-2h-8l-2-2z"></path>
        </svg>
      </div>
    {{else}}
